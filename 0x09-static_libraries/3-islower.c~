#include <unistd.h>
#include "main.h"

/**
 * _islower - Checks for lowercase character
 * Description: It prints the alphabet in lowercase fallowed by a new line
 * Return: Always 0
 */

void print_alphabet_x10(void)
{
	int _islower(int c)
	{
		if (c >= 97 && c <= 122)
		{
			return (1);
		}
		return (0);
}
